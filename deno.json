{
  "tasks": {
    "dev": "deno run -A --watch src/main.tsx",
    "build": "deno run -A build.ts",
    "preview": "deno run -A --port=5173 server.ts"
  },
  "imports": {
    "react": "https://esm.sh/react@18.2.0?pin=v135",
    "react/jsx-runtime": "https://esm.sh/react@18.2.0/jsx-runtime?pin=v135",
    "react-dom": "https://esm.sh/react-dom@18.2.0?pin=v135",
    "react-dom/client": "https://esm.sh/react-dom@18.2.0/client?pin=v135",
    "react-router-dom": "https://esm.sh/react-router-dom@6.8.1?pin=v135",
    "chart.js": "https://esm.sh/chart.js@4.2.1?pin=v135",
    "react-chartjs-2": "https://esm.sh/react-chartjs-2@5.2.0?pin=v135"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  }
}